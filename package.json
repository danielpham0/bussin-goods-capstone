{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Starter kit for creating React and Express apps",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Bussin' Devs",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^5.2.7",
    "body-parser": "^1.19.2",
    "concurrently": "^3.5.1",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "dynamodb-store": "^1.2.0",
    "express": "^4.16.2",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "mongoose": "^6.2.4",
    "ngrok": "^4.3.0",
    "sessions": "^0.0.2-7",
    "stripe": "^8.205.0"
  }
}
